<div class="jumbotron">
  <div class="container">
    <h1 class="display-4"><%= @dojot.first_name %> <%= @dojot.last_name %></h1>
    <p class="lead"><%= @dojot.email %> | <%= @dojot.phone %> | <%= @dojot.twitter %></p>
  </div>
  <hr>
  <%= link_to "Edit", edit_dojot_path(@dojot),class:"btn btn-sm btn-secondary" %>
